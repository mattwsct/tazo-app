# Kick Integration (optional)
KICK_CHANNEL_NAME=your_kick_channel_name_here
KICK_BOT_TOKEN=your_kick_bot_token_here

# App Configuration
NEXT_PUBLIC_APP_URL=https://app.tazo.wtf
NODE_ENV=development 