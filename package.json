    "fix-icloud": "bash scripts/fix-icloud-sync.sh"
  },
  "dependencies": {
    "@vercel/kv": "^3.0.0",
    "maplibre-gl": "^5.7.1",
    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "^16.1.6",
    "typescript": "^5"
  }
}
